{
  "name": "boilerplate-frontend",
  "version": "1.0.0",
  "description": "Boilerplate Frontend",
  "scripts": {
    "prebuild": "node prebuild.js",
    "ng": "ng",
    "start": "ng serve --port=4215 --host=0.0.0.0",
    "build": "ng version; ng build --prod",
    "lint": "eslint . --ext .ts",
    "lint:json": "eslint . --ext .ts -f json -o coverage/eslint.json",
    "lint:fix": "eslint . --fix --ext .ts",
    "clean": "rm -rf build coverage",
    "view:coverage": "serve coverage/lcov-report",
    "sonar": "sonar-scanner",
    "test": "cross-env NODE_ENV=test jest --coverage --testTimeout=10000 && npm run lint:json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/didux-io/Boilerplate-Frontend.git"
  },
  "bugs": {
    "url": "https://github.com/didux-io/Boilerplate-Frontend/issues"
  },
  "homepage": "https://www.didux.io",
  "author": "Didux.io",
  "license": "GPL-3.0-only",
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "clearMocks": true,
    "testResultsProcessor": "jest-sonar-reporter",
    "roots": [
      "src"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "jest-report.xml",
    "indent": 4
  },
  "private": true,
  "browser": {
    "http": false,
    "https": false,
    "net": false,
    "path": false,
    "stream": false,
    "tls": false,
    "fs": false,
    "zlib": false
  },
  "dependencies": {
    "@angular/animations": "10.2.1",
    "@angular/cdk": "^11.0.0",
    "@angular/common": "10.2.1",
    "@angular/compiler": "10.2.1",
    "@angular/core": "11.0.5",
    "@angular/forms": "10.2.1",
    "@angular/platform-browser": "10.2.1",
    "@angular/platform-browser-dynamic": "10.2.1",
    "@angular/router": "10.2.1",
    "@fortawesome/angular-fontawesome": "0.7.0",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "5.15.1",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@ng-bootstrap/ng-bootstrap": "7.0.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@ngxs-labs/async-storage-plugin": "0.1.1",
    "@ngxs/store": "3.7.0",
    "@proofmeid/webrtc-web": "0.0.12",
    "@types/jwt-decode": "2.2.1",
    "@types/qrcode": "1.3.5",
    "bootstrap": "4.5.3",
    "jwt-decode": "3.0.0",
    "ngx-bootstrap": "^6.1.0",
    "ngx-device-detector": "^2.0.0",
    "ngx-easy-table": "^13.0.3",
    "ngx-select-ex": "4.0.0",
    "ngx-toastr": "13.1.0",
    "qrcode": "1.4.4",
    "recaptcha-v3": "^1.8.0",
    "rxjs": "6.6.3",
    "rxjs-compat": "6.6.3",
    "websocket": "1.0.32",
    "wrtc": "0.4.6",
    "zone.js": "0.11.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1002.0",
    "@angular/cli": "10.2.0",
    "@angular/compiler-cli": "10.2.1",
    "@ngxs/devtools-plugin": "3.7.0",
    "@types/jest": "^26.0.15",
    "@types/node": "14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "codelyzer": "6.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.13.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "i": "^0.3.6",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "jest-sonar-reporter": "^2.0.0",
    "mocha": "^8.2.1",
    "npm": "^6.14.8",
    "serve": "^11.3.2",
    "sonar-scanner": "^3.1.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "9.0.0",
    "typescript": "4.0.5"
  }
}
